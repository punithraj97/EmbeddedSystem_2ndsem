package lambda;

import java.util.*;


public class MultipleThreads extends Thread implements Runnable {
	
	static int arr[];
	static char arr1[];
	
	public MultipleThreads()
	{
		
	}
	
	public static void sort(Thread t,int arr1[])
	{
		arr=arr1;
		t.start();
	}
	public static void sort1(Thread t,int arr1[])
	{
		arr=arr1;
		t.start();
	}
	
	public void run() {
		
		Arrays.sort(arr);
	}}
