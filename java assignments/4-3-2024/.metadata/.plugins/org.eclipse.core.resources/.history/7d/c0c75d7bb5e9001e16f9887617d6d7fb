package lambda;

import java.util.*;


public class MultipleThreads extends Thread implements Runnable {
	
	static int arr[];
	
	public MultipleThreads()
	{
		
	}
	
	public static void(Thread t,int arr1[])
	{
		arr=arr1;
	}
	
	public void run() {
		
		Arrays.sort(arr);
	}

	public static void main(String[] args) {
		

	}

}
